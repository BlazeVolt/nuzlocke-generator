{
  "name": "nuzlocke-generator",
  "version": "0.0.1",
  "description": "A tool for generating nuzlocke team pics from data",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --config jest-config.json",
    "prebuild": "npm run lint",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "styleguide": "styleguidist server --config styelguide.config.js --verbose",
    "styleguide:build": "styleguidist build --config styelguide.config.js --verbose",
    "clean": "rimraf dist",
    "pretest": "npm run lint",
    "lint": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "prestart": "webpack",
    "serve": "ts-node --disableWarnings server-src",
    "start": "npm run serve"
  },
  "keywords": [],
  "author": "Emma Ramirez <ramirez.emma.g@gmail.com> (http://emmaramirez.me)",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^1.27.0",
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^2.8.7",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/pure-render-decorator": "^0.2.28",
    "@types/react": "^16.0.5",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.7",
    "@types/react-router-dom": "^4.0.7",
    "@types/redux-logger": "^3.0.3",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "imports-loader": "^0.7.1",
    "jest": "^21.0.2",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^3.8.2",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "mocha": "^3.5.1",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-docgen-typescript": "^1.0.2",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-resizable": "^1.7.4",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "^6.0.24",
    "react-test-renderer": "^15.6.1",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.3.6",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "uuid": "^3.0.1",
    "webpack": "^2.7.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|styl)$": "<rootDir>stylus.config.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
