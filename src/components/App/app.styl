.release-notes-wrapper
    .release-notes
        background: #fafafa;
        margin: .5rem;
        padding: 1rem;
    h1, h2, h3, h4
        color: #333 !important;
    h1
        font-size: 1.5rem !important;
    p
        line-height: 1.4;
        margin-top: .5rem;

.release-dialog
    width: 620px !important;

.pt-tag-input .pt-tag.pt-tag-removable
    color: unset;

.pt-tag.pt-tag-removable,
.move
    color: #000;
    &.Fighting-type 
        background: brown;
        color: #eee;
    &.Ice-type 
        background: #C2F1F2;
    &.Fire-type 
        background: #EB3434;
    &.Grass-type 
        background: #39BF3C
    &.Bug-type 
        background: #AEE359;
    &.Normal-type 
        background: #E1E3D3;
    &.Steel-type 
        background: #B0B0B0;
    &.Rock-type 
        background: #8F795D;
    &.Ground-type 
        background: #9C6E21;
    &.Poison-type 
        background: #75226B;
        color: #eee;
    &.Ghost-type 
        background: #3B2238 !important;
        color: #eee;
    &.Water-type 
        background: #5B64DE;
        color: #eee;
    &.Electric-type 
        background: #E3E039;
    &.Dark-type 
        background: #29291F;
        color: #eee;
    &.Fairy-type 
        background: pink;
    &.Flying-type 
        background: #96D3E0;
    &.Psychic-type 
        background: #EB75DD;
    &.Dragon-type 
        background: #153F4D;
        color: white;
    &.-type 
        display: none;